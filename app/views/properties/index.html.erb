<% @title = "Properties" %>
<div class="span12">
   <h4>Properties</h4>
   <% if current_client.try(:admin?) %>
   <p>List of properties I am managing
   <% else %>
   <p>Your property manager wants you check these out!</p>
   <% end %>

   <table class="table table-hover">
   <%= render @properties %>
   </table>

   <% if current_client.try(:admin?) %>
   <div class="form-actions">
   <%= link_to 'Create Property', new_property_path, :class => 'btn btn-primary' %>
   </div>
   <% end %>

</div>
